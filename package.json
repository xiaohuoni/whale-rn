{
  "name": "whale-rn",
  "version": "0.0.1",
  "description": "yes! this is a react-native components",
  "main": "index.js",
  "directories": {
    "components": "components"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write --single-quote --semi --trailing-comma es5 --print-width 80 \"rn/components/**/*.js\"",
    "lint": "eslint rn/components",
    "format": "concurrently \"npm run prettier\" \"npm run lint --fix\"",
    "precommit": "npm run format",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "umi dev",
    "startRn": "node node_modules/react-native/local-cli/cli.js start --projectRoots rn",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --project-path rn/ios"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaohuoni/whale.git"
  },
  "keywords": [
    "ui",
    "ui-component",
    "component",
    "react",
    "react-native",
    "react-native-component"
  ],
  "files": [
    "components"
  ],
  "authors": [
    "xiaohuoni <448627663@qq.com> (https://github.com/xiaohuoni)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaohuoni/whale/issues"
  },
  "homepage": "https://github.com/xiaohuoni/whale#readme",
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "4.0",
    "concurrently": "^4.0.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "path": "^0.12.7",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.5.0",
    "through2": "^2.0.3",
    "vinyl-fs": "^3.0.3"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-art": "^16.5.2",
